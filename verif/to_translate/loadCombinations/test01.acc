\path{"/usr/local/lib/macros_lcmd"}

\comb_acciones
  {
    \incluye{"comb_acciones/eae/gammaf_eae.cmbm"}
    
    \pond_acciones
      {
        \EAE
          {
            \permanentes
              {
                \acciones
                  {
                    \accion
                      {
                        \nombre{"G1"} \descripcion{"Peso propio y carga muerta"}
                      }
                  }
              }
          }
      }
    
    \genera_combinaciones{}
    \expr{nCP= (@combinaciones.comb_elu_persistentes).num_combinaciones}
    \expr{nCA= (@combinaciones.comb_elu_accidentales).num_combinaciones}
    \expr{nCS= (@combinaciones.comb_elu_sismicas).num_combinaciones}
    \expr{nCPF= (@combinaciones.comb_els_poco_frecuentes).num_combinaciones}
    \expr{nCF= (@combinaciones.comb_els_frecuentes).num_combinaciones}
    \expr{nCCP= (@combinaciones.comb_els_cuasi_permanentes).num_combinaciones}
    
    \expr{ratio1= nCP-2}
    \expr{ratio2= nCPF-1}
    \expr{ratio3= nCF-1}
    \expr{ratio4= nCCP-1}
    
    \c{
    \print{"No. comb. persistentes: ",nCP,"\n"}
    \print{"No. comb. accidentales: ",nCA,"\n"}
    \print{"No. comb. s√≠smicas: ",nCS,"\n"}
    \print{"No. comb. poco frecuentes: ",nCPF,"\n"}
    \print{"No. comb. frecuentes: ",nCF,"\n"}
    \print{"No. comb. cuasi permanentes: ",nCCP,"\n"}
    }
    
    \if
      {
        \cond{(ratio1 < 1e-12) & (nCA < 1e-12) & (nCS < 1e-12) & (ratio2 < 1e-12) & (ratio3 < 1e-12) & (ratio4 < 1e-12)}
        \then{\print{"test_01: pasa.\n"}}
        \else{\print{"test_01: falla.\n"}}
      }
  }