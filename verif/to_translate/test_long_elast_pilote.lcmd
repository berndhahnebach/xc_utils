\path{"/usr/local/lib/macros_lcmd"}
\incluye{"geotecnia/cimientos/long_elastica_pilote.lcmm"}
\def_prop["Ep","double"]{3.19E+010} \c{Módulo elástico del hormigón del pilote.}
\def_prop["Ip","double"]{1/12*0.27^4} \c{Módulo elástico del hormigón del pilote.}
\def_prop["Et","double"]{4.41E+006} \c{Módulo elástico del terreno.}

\def_prop["Le","double"]{@longElasticaPiloteArcillas(@Ep,@Ip,@Et,25)}
\def_prop["Lp","double"]{@longEmpotramientoPiloteArcillas(@Ep,@Ip,@Et,25)}

\expr{ratio1= @tonum((@Le-1.76065)/1.76065)}
\expr{ratio2= @tonum((@Lp-2.11278)/2.11278)}

\c{
\print{"Le= ",@Le,"\n"}
\print{"ratio1= ",@tonum(ratio1),"\n"}
\print{"ratio2= ",@tonum(ratio2),"\n"}
  }
\if
  {
    \cond{(@abs(ratio1)<1e-4) & (@abs(ratio2)<1e-4)}
    \then{\print{"test longitud elastica pilote 01: ok.\n"}}
    \else{\print{"test longitud elastica pilote 01: ERROR.\n"}}
  }